vector Bytes <byte>;

table BurnId {
    burnTxHash: Bytes,
    logIndex: Bytes,
}

vector BurnIds <BurnId>;

// Used in unlock-asset-tx witness when bridge Nervos assets to Xchain
table UnlockMemo {
    xchain: byte,  // 1 = Ethereum
    burnIds: BurnIds,   // if xchain == 1, burnIds = [{burnTxHash, logIndex}]
}
